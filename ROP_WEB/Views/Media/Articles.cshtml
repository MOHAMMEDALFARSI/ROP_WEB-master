@using ROP_WEB.Res


<div class="container TopicStyle">
    <div class="row">

        <div class="col breadcrumbstyle" style="padding:0">
            <nav>
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="../../home/index">@Resource.home</a></li>
                    <li class="breadcrumb-item">@Resource.media</li>
                    <li class="breadcrumb-item active">@Resource.md2  </li>

                    <li class="readspeakercls">
                        <div id="readspeaker_button1" class="rs_skip rsbtn rs_preserve" style="margin-bottom:0;">
                            <a rel="nofollow" class="rsbtn_play" accesskey="L" title="@Resource.listen2" href="https://app-as.readspeaker.com/cgi-bin/rsent?customerid=5953&amp;lang=@Resource.listen3lang&amp;readid=item1&amp;url=www%2Erop%2Egov%2Eom%2Farabic%2Fe-readiness%2Ehtml">
                                <span class="rsbtn_left rsimg rspart"><span class="rsbtn_text"><span>@Resource.listen</span></span></span>
                                <span class="rsbtn_right rsimg rsplay rspart"></span>
                            </a>
                        </div>
                    </li>
                </ol>
            </nav>
        </div>
    </div>



    <div class="container topiccontiner">

        <div class="row">

            <div class="col-md-4 d-none d-sm-block">
                <ul class="list-group">
                    <li class="list-group-item"> <a href="#">@Resource.md1  </a></li>
                    <li class="list-group-item activelist"><a href="#">@Resource.md2 </a></li>

                    <li class="list-group-item"><a href="#">@Resource.md4</a></li>
                    <li class="list-group-item"><a href="#">@Resource.md5</a></li>
                    <li class="list-group-item"><a href="#">@Resource.md6</a></li>
                    <li class="list-group-item"><a href="#">@Resource.md7</a></li>
               


                </ul>

            </div>
            <div class="col-md-8">
                <h3 style="text-align:center; padding:10px;">  @Resource.md2 </h3>
                <div style="padding:10px; padding-bottom:0;">
                 
                    <div class="page">
                        <h3>اجتماع أصحاب المعالي والسعادة</h3>
                        <div class="details">
                            <ul style="color:#b5b5b5">

                                <li>April 18, 2017</li>
                                <li>    النقيب/محمد الخروصي</li>

                            </ul>
                        </div>
                        <div class="content">
                            <p>
                                السلطنة تستضيف اجتماع أصحاب المعالي والسعادة وكلاء وزارات الداخلية

                            </p>


                        </div>
                        <h4><a href="NewsDetails"> @Resource.readmore</a></h4>

                    </div>

                    <ul class="pagination">
                        <li class="page-item"><a class="page-link" href="#">Previous</a></li>
                        <li class="page-item"><a class="page-link activelist" href="#">1</a></li>
                        <li class="page-item"><a class="page-link" href="#">2</a></li>
                        <li class="page-item"><a class="page-link" href="#">3</a></li>
                        <li class="page-item"><a class="page-link" href="#">Next</a></li>
                    </ul>
                </div>


            </div>
        </div>



    </div>
</div>

