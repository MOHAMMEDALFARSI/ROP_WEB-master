
@section PageTitle {
    @Resource.worktime
}
@section arurl{
    <a href="@Url.Action("GetLang", "Home", new { lang = "ar-OM", view="WorkTimes",cont="Contact"}  )">
        <i class="fa fa-globe"></i> عربي
    </a>
}

@section enurl{
    <a href="@Url.Action("GetLang", "Home", new { lang = "en-US",view="WorkTimes",cont="Contact"}  )">
        <i class="fa fa-globe"></i> En
    </a>
}
@{

    var url = Request.Url.ToString();
}

@using ROP_WEB.Res


<div class="container">
    <div class="row">

        <div class="col breadcrumbstyle" style="padding:0">
